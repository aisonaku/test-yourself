<div class="tys-quizzes">

<div class="tys-quizzes-title text-shortcut" {{action 'getUser'}}>Your profile</div>
{{#each model as |user|}}
         <div class="tys-question-results-title">{{user.name}}</div>
		 {{/each}}
		 
	  
      {{#list-filter filter=(action 'getResultForQuiz') as |answers|}}
      {{#each answers as |answer|}}
	  
         <div class="tys-quizzes-item">
		 {{#quiz-filter filter=(action 'getQuiz' answer.id) as |quiz|}}
		 <div class="tys-question-results-title" title="{{ quiz.title }}">{{ quiz.title }}</div>
            <div class="tys-quizzes-filter-category-item text-shortcut" title="{{ answer.title }}">{{ answer.value }} %</div>
         {{/quiz-filter}}
		 </div>
      {{/each}}
   {{/list-filter}}
</div>